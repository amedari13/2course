<UserControl x:Class="_7lab.user_control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_7lab"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <local:Numbers Number="1111" x:Key="num" />
    </UserControl.Resources>
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <TextBox Margin="65,37,65,7" Grid.Row="0" x:Name="txtb" TextChanged="txtb_TextChanged" KeyDown="txtb_KeyDown"/>

        <Button Margin="99,22,97,5" Background="Black" Foreground="White" Content="Check info" Grid.Row="1" Click="Button_Click"/>
    </Grid>
</UserControl>
